{
  "compilerOptions": {
	"version": true,
	"outDir": "dist",
	"declaration": true,
    "target": "ES5",
    "module": "system",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false
  },
  "exclude": [
	"dist",
    "node_modules"
  ]
}
